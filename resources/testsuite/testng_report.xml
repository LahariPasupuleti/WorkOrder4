<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite-Menus" parallel="tests">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	
	 <test name="Test-Manager-Menus">
		<parameter name="UserType" value="Manager"/>
		<classes>
			<class name="com.incessant.uiautomation.tests.TestReports">
				<methods>
					<include name="testGetReports"></include>
				</methods>
			</class> 
		</classes>
	</test> 
	
	 <test name="Test-User-Menus">
		<parameter name="UserType" value="User"/>
		<classes>
			<class name="com.incessant.uiautomation.tests.TestReports">
				<methods>
					<include name="testGetReports"></include>
				</methods>
			</class> 
		</classes>
	</test> 
</suite>